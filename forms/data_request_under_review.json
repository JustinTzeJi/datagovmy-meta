	"send_email": true,
	"validate_fields": ["ticket_id", "name", "dataset_title", "agency"],
	"email_template": [
		{
			"name": "data_request_under_review",
			"subject": "[Ticket ID: {{ticket_id}}] Data Request from data.gov.my",
			"content": "UNDER REVIEW {{name}} | {{dataset_title}} | {{agency}}",
			"html_content": "UNDER REVIEW {{name}} | {{dataset_title}} | {{agency}}",
			"language": "en-GB"
		},
		{
			"name": "data_request_under_review",
			"subject": "[Tiket ID: {{ticket_id}}] Permintaan Data dari data.gov.my",
			"content": "BAWAH REVIEW {{name}} | {{dataset_title}} | {{agency}}",
			"html_content": "BAWAH REVIEW {{name}} | {{dataset_title}} | {{agency}}",
			"language": "ms-MY"
		}
	]
}
